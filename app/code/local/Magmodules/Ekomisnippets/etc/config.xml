<config>
    <modules>
        <Magmodules_Ekomisnippets>
            <version>1.1.3</version>
        </Magmodules_Ekomisnippets>
    </modules>
    <global>
        <helpers>
            <ekomisnippets>
                <class>Magmodules_Ekomisnippets_Helper</class>
            </ekomisnippets>
        </helpers>
        <blocks>
            <ekomisnippets>
                <class>Magmodules_Ekomisnippets_Block</class>
            </ekomisnippets>
        </blocks>
        <models>
            <ekomisnippets>
                <class>Magmodules_Ekomisnippets_Model</class>
            </ekomisnippets>
        </models>
    </global>
    <frontend>
        <layout>
            <updates>
                <magmodules_ekomisnippets>
                    <file>magmodules_ekomisnippets.xml</file>
                </magmodules_ekomisnippets>
            </updates>
        </layout>
        <translate>
            <modules>
                <magmodules_ekomisnippets>
                    <files>
                        <default>Magmodules_Ekomisnippets.csv</default>
                    </files>
                </magmodules_ekomisnippets>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <magmodules_ekomisnippets>
                    <files>
                        <default>Magmodules_Ekomisnippets.csv</default>
                    </files>
                </magmodules_ekomisnippets>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <ekomi_snippets>
            <general>
                <enabled>0</enabled>
            </general>
        </ekomi_snippets>
    </default>
</config>